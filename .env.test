# Telegram Bot Token (placeholder)
BOT_TOKEN=placeholder_token

# Database (SQLite for testing)
DATABASE_URL=sqlite:///./mysticbot.db

# Redis (empty to use memory storage)
REDIS_URL=

# API Keys (optional)
OPENAI_API_KEY=
PERPLEXITY_API_KEY=

# Logging
LOG_LEVEL=DEBUG

# Paths
UPLOAD_DIR=uploads
DATA_DIR=data

# Rate limiting
RATE_LIMIT=30
RATE_WINDOW=60

# Enable modules
ENABLE_TAROT=true
ENABLE_PALMISTRY=false
ENABLE_NUMEROLOGY=true
ENABLE_HOROSCOPE=true
ENABLE_FINANCE_CALENDAR=true